# Configuration de la base de données MongoDB
# Remplace par ta vraie chaîne de connexion MongoDB Atlas
URI_MONGO=mongodb+srv://username:password@cluster.mongodb.net/database_name?retryWrites=true&w=majority

# JWT Secret pour l'authentification
# Génère un secret aléatoire fort (ex: openssl rand -base64 32)
JWT_SECRET=your_super_secret_jwt_key_here

# Port du serveur (optionnel, par défaut 8888)
PORT=8888

# Environnement d'exécution
NODE_ENV=development
